    "def mean(xs):\n",
    "    \"\"\"\n",
    "    This function calculates the sum\n",
    "    of the elements in the array xs.\n",
    "    \"\"\"\n",
    "    sum = 0\n",
    "    for x in xs:\n",
    "        sum += x\n",
    "    return sum / len(xs)\n",
    "\n",
    "print(mean([1, 2, 3, 4, 5]))\n",
    "#> 3"
   ]
  }
